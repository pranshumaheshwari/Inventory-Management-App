<% include partials/header.ejs %>
<br><br>
<h1 align="center">Excess On Line Report</h1>
<br><br>
<div class="container-fluid">
  <table class="table table-striped table-bordered table-hover">
    <thead class="thead-dark">
      <th>Part Number</th>
      <th>Description</th>
      <th>DTPL Part Number</th>
      <th>Current Store Stock</th>
      <th>Current Line Stock</th>
      <th>Excess On Line</th>
    </thead>
    <tbody>
      <% for(var i=0;i<data.length;i++){ %>
        <tr>
            <td><%= data[i].code %></td>
            <td><%= data[i].name %></td>
            <td><%= data[i].DTPL_code %></td>
            <td><%= data[i].stock %></td>
            <td><%= data[i].line_stock %></td>
            <td><%= data[i].excessQuantity %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>
<script type="text/javascript">
	new TableExport(document.getElementsByTagName("table"),{bootstrap:false,headers:true,formats:['csv','xls','xlsx'],ignoreCols:[]});
</script>
<% include partials/footer.ejs %>
